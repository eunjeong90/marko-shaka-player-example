{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "lib": ["es2018", "dom"]
  },
  "include": ["src/**/*", ".marko-run/**/*", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "script-lang": "ts"
}
